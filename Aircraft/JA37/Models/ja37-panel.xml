    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.31 </y-m>
      <z-m>  0.17 </z-m>
    </offsets>
  </model>

  <animation>
    <type>rotate</type>
    <object-name>handleRight</object-name>
    <offset-deg>90</offset-deg>
    <center>
     <x-m>  0.00 </x-m>
     <y-m>  0.31 </y-m>
     <z-m>  0.17 </z-m>
    </center>
    <axis>
     <x>1</x>
     <y>0</y>
