    <offsets>
      <x-m>0</x-m>
      <y-m>0.29</y-m>
      <z-m>0.035</z-m>
    </offsets>
  </model>

  <animation>
    <type>scale</type>
    <object-name>Augmentation Indicator</object-name>
    <x-offset>4.00</x-offset>
    <y-offset>1.00</y-offset>
    <z-offset>1.00</z-offset>
  </animation>

  <model>
    <path>Aircraft/JA37/Models/Cockpit/handle.xml</path>
