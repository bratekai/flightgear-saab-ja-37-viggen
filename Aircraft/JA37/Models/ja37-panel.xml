      <y-m>  0.1225</y-m>
      <z-m>  0.30</z-m>
    </offsets>
  </model>

  <animation>
    <type>scale</type>
    <object-name>ADIj</object-name>
    <x-offset>1.05</x-offset>
    <y-offset>1.05</y-offset>
    <z-offset>1.05</z-offset>
