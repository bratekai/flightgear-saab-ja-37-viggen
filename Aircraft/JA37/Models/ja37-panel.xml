<?xml version="1.0"?>
<PropertyList>
  <!-- right side -->
  <model>
    <name>MI</name>
    <path>Aircraft/JA37/Models/Cockpit/MI/mi.xml</path>
    <offsets>
      <x-m> 0.000</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.0475</z-m>
    </offsets>
  </model>

  <model>
    <name>TI</name>
    <path>Aircraft/JA37/Models/Cockpit/TI/ti.xml</path>
    <offsets>
      <x-m> 0.000</x-m>
      <y-m> 0.195</y-m>
      <z-m> 0.12</z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/JA37/Models/Instruments/Fuel/fuel_ja.xml</path>
